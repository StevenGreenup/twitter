<%= form_for @user, url: create_user_path do |f| %>
<%= f.label :username %>
<%= f.text_field :username %><br>
<%= f.label :photo_url %>
<%= f.text_field :photo_url %><br>
<%= f.label :password %>
<%= f.password_field :password %><br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.submit %>
<% end %>
